<tr id="device_<%= device.id %>">
  <td class="align-middle"><%= device.name %></td>
  <td class="align-middle"><%= l(device.last_authenticated_at, format: :long) %></td>
  <td class="align-middle"><%= button_to 'Delete', user_fido_usf_registration_path(id: device.id), class: 'btn btn-sm btn-outline-danger', remote: true, :method => :delete, data: {confirm: "Should device #{device.name} be deleted?" } %></td>
</tr>
